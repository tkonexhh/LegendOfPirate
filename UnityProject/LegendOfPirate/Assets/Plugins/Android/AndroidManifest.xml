    <!-- DC_APPID 是一组32位的代码，可以在g.dataeye.com创建游戏后获得. -->
    <!-- “YOUR_APPID”的部分要按照实际情况设置，一定要记得替换哦 -->
    <meta-data android:value="94136BDFFD9A1086EAECFFBC64AB26A8" android:name="DC_APPID" />
